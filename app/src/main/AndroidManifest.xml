<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.something.artur.RubikApp">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/r1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.something.artur.RubikApp.Principal"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.something.artur.RubikApp.Notacio" />
        <activity android:name="com.something.artur.RubikApp.Parts" />
        <activity android:name="com.something.artur.RubikApp.TutoMenu" />
        <activity android:name="com.something.artur.RubikApp.Tuto1" />
        <activity android:name="com.something.artur.RubikApp.Tuto2" />
        <activity android:name="com.something.artur.RubikApp.Tuto3" />
        <activity android:name="com.something.artur.RubikApp.Tuto4" />
        <activity android:name="com.something.artur.RubikApp.Tuto5" />
        <activity android:name="com.something.artur.RubikApp.Tuto6" />
        <activity android:name="com.something.artur.RubikApp.Tuto7" />
        <activity android:name="com.something.artur.RubikApp.Minijoc" />
        <activity android:name="com.something.artur.RubikApp.Categories" />
        <activity android:name="com.something.artur.RubikApp.Recursos" />
        <activity android:name="com.something.artur.RubikApp.Crono"></activity>
    </application>

</manifest>